{
  "name": "@devops-ia/release-cleaner",
  "version": "3.0.0",
  "description": "GitHub Action for Helm Release Cleaner",
  "author": "amartingarcia <adrianmg231189@gmail.com> (https://github.com/devops-ia/release-cleaner)",
  "homepage": "https://github.com/devops-ia/release-cleaner#readme",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "release-cleaner",
    "action"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devops-ia/release-cleaner/issues"
  },
  "release": {
    "tagFormat": "v${version}",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release": "^19.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@semantic-release/git": "^10.0.1"
  }
}